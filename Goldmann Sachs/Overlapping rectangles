def doOverlap(self,l1,r1,l2,r2):
        '''
         just compare 
         1. top x of 1 with least x of 2 
         2. top y of 1 with least y of 2
        '''
        if r2[0] < l1[0] or r1[0] < l2[0]:
            return 0
        if r2[1] > l1[1] or l2[1] < r1[1]:
            return 0
        return 1
